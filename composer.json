{
    "name": "2amigos/qrcode-library",
    "description": "QrCode Generator",
    "type": "library",
    "keywords": [
        "2amigos",
        "extension",
        "component",
        "qrcode",
        "qr"
    ],
    "homepage": "https://2am.tech/our-work/open-source/qrcode-library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "https://2am.tech",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/qrcode-library/issues",
        "source": "https://github.com/2amigos/qrcode-library"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-gd": "*",
        "ext-simplexml": "*",
        "bacon/bacon-qr-code": "^3.0.0",
        "khanamiryan/qrcode-detector-decoder": "^2.0.0",
        "marc-mabe/php-enum": "^4.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "friendsofphp/php-cs-fixer": "^v3.35",
        "codeception/specify": "^2.0.0",
        "codeception/verify": "^3.0 || ^2.2",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "yiisoft/yii2": "^2.0",
        "codeception/module-filesystem": "^3.0 || ^2.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-laravel": "^3.2.0",

        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Da\\QrCode\\": "./src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Da\\QrCode\\Providers\\QrCodeServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
